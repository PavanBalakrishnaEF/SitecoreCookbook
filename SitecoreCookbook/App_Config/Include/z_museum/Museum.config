<?xml version="1.0"?>
<configuration  xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <sites>
      <site name="Museum"
           enableTracking="true"
           virtualFolder="/"
           physicalFolder="/"
           rootPath="/sitecore/content/"
           startItem="/Museum"
           language="en"
           database="web"
           domain="extranet"
           hostName="www.museum.com"
           allowDebug="true"
           cacheHtml="true"
           htmlCacheSize="50MB"
           registryCacheSize="0"
           viewStateCacheSize="0"
           xslCacheSize="25MB"
           filteredItemsCacheSize="10MB"
           enablePreview="true"
           enableWebEdit="true"
           enableDebugger="true"
           disableClientData="false"
           cacheRenderingParameters="true"
           renderingParametersCacheSize="10MB"
           patch:before="site[@name='website']"
           linkProvider="link-pattern-1"
      />
      <site name="qa-Museum"
     enableTracking="true"
     virtualFolder="/"
     physicalFolder="/"
     rootPath="/sitecore/content/"
     startItem="/Museum"
     language="en"
     database="master"
     domain="extranet"
     hostName="qa.museum.com"
     allowDebug="true"
     cacheHtml="true"
     htmlCacheSize="50MB"
     registryCacheSize="0"
     viewStateCacheSize="0"
     xslCacheSize="25MB"
     filteredItemsCacheSize="10MB"
     enablePreview="true"
     enableWebEdit="true"
     enableDebugger="true"
     disableClientData="false"
     cacheRenderingParameters="true"
     renderingParametersCacheSize="10MB"
     patch:before="site[@name='website']"
     linkProvider="link-pattern-1"
      />

    </sites>
  </sitecore>
</configuration>
